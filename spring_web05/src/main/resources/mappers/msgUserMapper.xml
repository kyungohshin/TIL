<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.joywins.mapper.MsgUserMapper">

	<update id="updatePoint">
		update ztbl_msg_user 
			set UPOINT_TTL = UPOINT_TTL + #{point} 
			where uid = #{uid}
	</update>

</mapper>